<h2>Feedback Table</h2>

<div class="container">
  <div class="row">
    <div class="col-xs-12">
      <table summary="This table shows Feedbacks" class="table table-bordered table-hover dt-responsive" >
        <thead>
          <tr>
            <th>Username</th>
            <th>Comments</th>
          </tr>
        </thead>
        <tbody>

                {{#each data}}
                <tr> <td>{{this.username}}</td><td> {{this.comment}}</td></tr>
                {{/each}}


      </tbody>
        <tfoot>
          <tr>
            <td colspan="2" class="text-center"></td>
          </tr>
        </tfoot>
      </table>
    </div>
  </div>
</div>

